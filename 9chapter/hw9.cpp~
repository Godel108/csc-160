#include <iostream>
#include <string>

using namespace std;

struct nameType
{
  string first;
  string last;
};

struct courseType
{
  string name;
  int callNum;
  int credits;
  char grade;
};


struct studentType
{
  nameType name;
  double gpa;
  courseType course;
};

studentType student;
studentType classList[100];
courseType course;
nameType name;


main ()
{
  course.name = "Programming I";
  course.callNum = 13452;
  course.credits = 3;
  course.grade = "";
  return (0);
}
